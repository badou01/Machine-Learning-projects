        "## let's apply some Undersampling techniques first"
      ],
      "metadata": {
        "id": "-p8Eex7WATqz"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from imblearn.under_sampling import RandomUnderSampler\n",
        "rus=RandomUnderSampler(sampling_strategy='auto',random_state=42)\n",
        "X=data.drop(\"default payment next month\",axis=1)\n",
        "y=data[\"default payment next month\"]\n",
        "X_rus_res,y_rus_res=rus.fit_resample(X,y)"
      ],
